{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@nrwl-airlines/api-interfaces": ["libs/api-interfaces/src/index.ts"],
      "@nrwl-airlines/check-in/feature-shell": [
        "libs/check-in/feature-shell/src/index.ts"
      ],
      "@nrwl-airlines/check-in/data-access": [
        "libs/check-in/data-access/src/index.ts"
      ],
      "@nrwl-airlines/shared/seatmap/feature-seatmap": [
        "libs/shared/seatmap/feature-seatmap/src/index.ts"
      ],
      "@nrwl-airlines/shared/seatmap/data-access": [
        "libs/shared/seatmap/data-access/src/index.ts"
      ],
      "@nrwl-airlines/shared/data-access": [
        "libs/shared/data-access/src/index.ts"
      ],
      "@nrwl-airlines/booking/data-access": [
        "libs/booking/data-access/src/index.ts"
      ],
      "@nrwl-airlines/booking/feature-shell": [
        "libs/booking/feature-shell/src/index.ts"
      ],
      "@nrwl-airlines/shared/util/auth": ["libs/shared/util/auth/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
